{
  "name": "abcjs",
  "version": "5.6.2",
  "description": "Renderer for abc music notation",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run fix-versions && npm run build:basic && npm run build:midi && npm run build:plugin && npm run build:plugin-midi && npm run build:grease",
    "build:basic": "webpack --env.type=basic",
    "build:midi": "webpack --env.type=midi",
    "build:plugin": "webpack --env.type=plugin",
    "build:plugin-midi": "webpack --env.type=plugin-midi",
    "build:grease": "cat src/plugin/greasemonkey.js bin/abcjs_plugin_5.6.2-min.js > bin/abcjs_plugin_5.6.2.user.js",
    "fix-versions": "./fix-versions.sh 5.6.2",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulrosen/abcjs.git"
  },
  "keywords": [
    "abc",
    "music",
    "notation",
    "midi"
  ],
  "author": "Paul Rosen <paul@paulrosen.net> (https://abcjs.net)",
  "contributors": [
    "Gregory Dyke",
    "Thomas Chandelle"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paulrosen/abcjs/issues"
  },
  "homepage": "https://abcjs.net",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.2.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^3.12.0"
  },
  "dependencies": {
    "midi": "https://github.com/paulrosen/MIDI.js.git#abcjs"
  }
}
