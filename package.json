{
  "name": "abcjs",
  "version": "6.0.0-beta.28",
  "description": "Renderer for abc music notation",
  "main": "index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "webpack": "webpack",
    "build": "npm run fix-versions && webpack --config-name basic --mode development && webpack --mode=production && npm run copy-output",
    "build:basic": "npm run webpack -- --config-name basic --mode development",
    "build:basic-min": "npm run webpack -- --config-name basic --mode production",
    "build:plugin": "npm run webpack -- --config-name plugin --mode production",
    "build:midi": "npm run webpack -- --config-name midi --mode production",
    "fix-versions": "./fix-versions.sh 6.0.0-beta.28",
    "copy-output": "./build-copy.sh 6.0.0-beta.28",
    "test": "mocha 'tests/**/*.js'",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "build:analyze": "npm run webpack -- --config-name basic --mode development --env analyze",
    "build:analyze": "npm run build:basic -- --env presets=analyze"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulrosen/abcjs.git"
  },
  "keywords": [
    "abc",
    "music",
    "notation",
    "midi",
    "webaudio"
  ],
  "author": "Paul Rosen <paul@paulrosen.net> (https://abcjs.net)",
  "contributors": [
    "Gregory Dyke",
    "Todd Brown",
    "Thomas Chandelle"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paulrosen/abcjs/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/paulrosen"
  },
  "homepage": "https://abcjs.net",
  "devDependencies": {
    "@babel/core": "7.12.16",
    "@babel/preset-env": "7.12.16",
    "@tarp/require": "1.4.3",
    "@vuepress/shared-utils": "1.5.4",
    "babel-loader": "8.2.2",
    "chai": "4.2.0",
    "compression-webpack-plugin": "5.0.1",
    "mocha": "8.1.3",
    "serialize-javascript": "4.0.0",
    "svg-inline-loader": "0.8.2",
    "vuepress": "1.5.4",
    "vuex": "3.6.0",
    "webpack": "5.21.2",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-cli": "4.5.0",
    "webpack-merge": "5.7.3",
    "webpack-node-externals": "2.5.2",
    "workbox-webpack-plugin": "5.1.3"
  },
  "peerDependencies": {
    "midi": "https://github.com/paulrosen/MIDI.js.git#abcjs"
  }
}
